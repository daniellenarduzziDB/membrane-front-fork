{"ast":null,"code":"import { useEffect } from 'react';\nexport default function useOutsideClick(ref, callback) {\n  useEffect(() => {\n    const handleClick = e => {\n      if (ref.current && !ref.current.contains(e.target)) callback();\n    };\n\n    document.addEventListener('click', handleClick);\n    return () => {\n      document.removeEventListener('click', handleClick);\n    };\n  }, [ref, callback]);\n}","map":{"version":3,"sources":["/home/juanma/projects/membrane-front/src/legacy/hooks/useOutsideClick.js"],"names":["useEffect","useOutsideClick","ref","callback","handleClick","e","current","contains","target","document","addEventListener","removeEventListener"],"mappings":"AAAA,SAASA,SAAT,QAA0B,OAA1B;AAEA,eAAe,SAASC,eAAT,CAAyBC,GAAzB,EAA8BC,QAA9B,EAAwC;AACrDH,EAAAA,SAAS,CAAC,MAAM;AACd,UAAMI,WAAW,GAAGC,CAAC,IAAI;AACvB,UAAIH,GAAG,CAACI,OAAJ,IAAe,CAACJ,GAAG,CAACI,OAAJ,CAAYC,QAAZ,CAAqBF,CAAC,CAACG,MAAvB,CAApB,EAAoDL,QAAQ;AAC7D,KAFD;;AAIAM,IAAAA,QAAQ,CAACC,gBAAT,CAA0B,OAA1B,EAAmCN,WAAnC;AACA,WAAO,MAAM;AACXK,MAAAA,QAAQ,CAACE,mBAAT,CAA6B,OAA7B,EAAsCP,WAAtC;AACD,KAFD;AAGD,GATQ,EASN,CAACF,GAAD,EAAMC,QAAN,CATM,CAAT;AAUD","sourcesContent":["import { useEffect } from 'react';\n\nexport default function useOutsideClick(ref, callback) {\n  useEffect(() => {\n    const handleClick = e => {\n      if (ref.current && !ref.current.contains(e.target)) callback();\n    };\n\n    document.addEventListener('click', handleClick);\n    return () => {\n      document.removeEventListener('click', handleClick);\n    };\n  }, [ref, callback]);\n}\n"]},"metadata":{},"sourceType":"module"}